{
  "name": "hybrid_vite",
  "private": true,
  "version": "0.0.3",
  "scripts": {
    "serve:pub": "cross-env PUBLIC_PATH= vite --mode publish --host",
    "serve:local": "cross-env PUBLIC_PATH= vite --mode testing --host",
    "serve:dev": "cross-env PUBLIC_PATH= vite --mode dev --host",
    "serve:product": "cross-env PUBLIC_PATH= vite --mode product --host",
    "build:local:pc": "cross-env PUBLIC_PATH=././ vite build --mode local_build ",
    "build:pub": "cross-env PUBLIC_PATH=././ vite build --mode publish_build",
    "build:dev:pc": "cross-env PUBLIC_PATH=././ vite build --mode dev",
    "build:product:pc": "cross-env PUBLIC_PATH=././ vite build --mode product",
    "build:local:app": "cross-env PUBLIC_PATH= RELEASE=false vite build --mode local",
    "build:dev:app:legacy": "cross-env PUBLIC_PATH= RELEASE=false vite build --mode dev",
    "build:dev:app": "cross-env PUBLIC_PATH= RELEASE=false vite build --mode dev",
    "build:product:app": "cross-env PUBLIC_PATH= RELEASE=true vite build --mode product",
    "test:local": "vitest --mode local",
    "test:dev": "vitest --mode dev",
    "test:product": "vitest --mode product",
    "coverage": "vitest run --coverage",
    "dev": "vite --mode dev",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --mode dev",
    "storybook": "storybook dev -p 2697",
    "build-storybook": "storybook build",
    "build:analyze": "npx vite-bundle-visualizer"
  },
  "dependencies": {
